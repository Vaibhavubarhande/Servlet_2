  </servlet>
  <servlet-mapping>
  	<servlet-name>math</servlet-name>
  	<url-pattern>/math</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>msg</servlet-name>
  	<url-pattern>/msg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
